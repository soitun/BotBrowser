{
    "applicationId": "io.botbrowser.launcher",
    "version": "1.0.0",
    "defaultMode": "window",
    "documentRoot": "/dist/botbrowser-launcher/browser",
    "url": "/",
    "enableServer": true,
    "enableNativeAPI": true,
    "nativeAllowList": [
        "app.*",
        "os.*",
        "storage.*",
        "window.*",
        "filesystem.*",
        "computer.*",
        "debug.*",
        "resources.*",
        "clipboard.*"
    ],
    "modes": {
        "window": {
            "title": "BotBrowser Launcher",
            "width": 1000,
            "height": 800,
            "minWidth": 400,
            "minHeight": 200,
            "center": true,
            "icon": "/dist/botbrowser-launcher/browser/favicon.ico",
            "enableInspector": false,
            "borderless": false,
            "maximize": false,
            "hidden": false,
            "resizable": true,
            "exitProcessOnClose": true,
            "enableNative": true,
            "useSavedState": true
        }
    },
    "cli": {
        "binaryName": "BotBrowserLauncher",
        "resourcesPath": "/dist/botbrowser-launcher/browser",
        "extensionsPath": "/extensions/",
        "binaryVersion": "6.5.0",
        "clientVersion": "6.5.0"
    }
}
